{
  "name": "@qwikdev/astro-node-biome-starter",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "npm run fix && astro build",
    "check": "biome ci . && astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "astro dev",
    "fix": "npm run lint && npm run format",
    "format": "biome format --write .",
    "lint": "biome check --apply-unsafe .",
    "preview": "astro preview",
    "prod": "npm run check && astro build",
    "serve": "node ./dist/server/entry.mjs",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/node": "^8.2.5",
    "@astrojs/react": "^3.6.1",
    "@builder.io/qwik": "https://pkg.pr.new/@builder.io/qwik@1cee8e6",
    "@qwikdev/astro": "^0.6",
    "astro": "^4.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0"
  }
}
